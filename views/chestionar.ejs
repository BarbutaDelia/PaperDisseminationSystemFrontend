<form action="/rezultat-chestionar" method="post">
    <% for (let i in intrebari) { 
        let c=0;%>
        <div class="question">
            <h3><%= intrebari[i].intrebare %></h3>
            <% for (const v in intrebari[i].variante) { %>
                <label><input type="radio" name = "q<%= i %>" value = "<%= c++ %>"/> <%= intrebari[i].variante[v] %></label>
            <% } %>
        </div>
    <% } %>
    <input type="submit" value="Trimite">

</form>